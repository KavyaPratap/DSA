#include <iostream>
using namespace std;
int main(){
  int arr[10]={22,11,34,23,56,76,87,112,89,99};
  int n=sizeof(arr)/sizeof(int);
  int elem;
  bool flag;
  cout << "Enter element to search: ";
  cin >>elem;
  for(int i=0;i<n-1;i++){
    if(arr[i]==elem)
      flag=true;
  }
  if (flag==true)
    cout<<"Element found"<<endl;
  else
    cout<<"Element not found"<<endl;
}
