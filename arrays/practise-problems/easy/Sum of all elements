/*
logic-->
initialize sum=0
iterate each element of array
sum=sum+arr[i]
*/
#include <iostream>
using namespace std;
int main(){
  int sum=0;
  int arr[] = {1, 2, 3, 4, 5};
  int n = sizeof(arr)/sizeof(arr[0]);
  for(int i=0;i<n-1;i++){
    sum=sum+arr[i];
  }
  cout << "sum of array elements are--> "<<sum;
}
